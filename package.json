{
  "name": "mobx-react-form",
  "private": true,
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm build:prepare && pnpm build:tsc && pnpm build:tsc-alias && pnpm build:copy && pnpm build:rsync",
    "build:prepare": "test -x .build && rm -rf .build; mkdir -p .build",
    "build:tsc": "tsc -p tsconfig.json --outDir .build",
    "build:tsc-alias": "tsc-alias -p tsconfig.json --outDir .build",
    "build:copy": "copyfiles -u 1 src/**/*.yml .build",
    "build:rsync": "rsync -uvaz --delete .build/ dist/; rm -rf .build",
    "clean": "rimraf dist",
    "test": "NODE_ENV=test jest"
  },
  "peerDependencies": {
    "mobx": "^6.0",
    "mobx-react": "^7.0",
    "react": "^18.0"
  },
  "dependencies": {
    "i18next": "^23.2.7",
    "lodash": "^4.17.21",
    "react-autofocus": "workspace:^",
    "react-timer": "workspace:^",
    "react-util": "workspace:^",
    "ytil": "workspace:^"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.0.4",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0",
    "vite-tsconfig-paths": "^4.0.7"
  }
}
